{
  "name": "spg-painter",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "rsw:build": "rsw && npm run build",
    "lint": "yarn eslint --ext js,ts,svelte ./*.js ./*.ts ./src",
    "lint-format": "yarn prettier -c ./*.json **/*.svelte",
    "lint:fix": "yarn lint --fix && yarn lint-format -w"
  },
  "dependencies": {
    "okageo": "^2.0.13",
    "okanvas": "^0.0.4",
    "ress": "^4.0.0",
    "svelte": "^3.42.5"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.22",
    "@tsconfig/svelte": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "@wasm-tool/rollup-plugin-rust": "^1.0.7",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.4.0",
    "prettier-plugin-svelte": "^2.4.0",
    "sass": "^1.39.2",
    "svelte-preprocess": "^4.9.4",
    "tslib": "^2.3.1",
    "typescript": "^4.3.2",
    "vite": "^2.5.0"
  }
}
